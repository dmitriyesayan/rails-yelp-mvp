<div class="card">
  <h2><%= @restaurant.name %></h2>
  <ul>
      <% @restaurant.reviews.each do |review| %>
      <li>
        <p><%= review.created_at.strftime("%c") %></p>
        <div>
          <p><%= review.rating %></p>
          <p><%= review.content %></p>
        </div>
      </li>
      <% end %>
  </ul>
</div>
